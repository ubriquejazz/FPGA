ifeq ($(MAKECMDGOALS),Blinkall_Debug)

Blinkall_Debug : ./Blinkall.dxe 

./debug/benchmark.doj :./benchmark.asm $(VDSP)/211xx/include/asm_sprt.h $(VDSP)/211xx/include/def21160.h 
	@echo ".\benchmark.asm"
	$(VDSP)/easm21k.exe .\benchmark.asm -proc ADSP-21160 -o .\debug\benchmark.doj -MM

debug/Blink.doj :Blink.c $(VDSP)/211xx/include/def21160.h $(VDSP)/211xx/include/21160.h $(VDSP)/211xx/include/filter.h $(VDSP)/211xx/include/complex.h 
