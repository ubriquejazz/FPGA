	$(VDSP)/cc21k.exe -c .\Blink.c -file-attr ProjectName=Blinkall -g -no-multiline -double-size-32 -warn-protos -21160rev0 -proc ADSP-21160 -o .\debug\Blink.doj -MM

./Blinkall.dxe :$(VDSP)/211xx/ldf/ADSP-21160.ldf $(VDSP)/211xx/lib/160_hdr.doj ./debug/benchmark.doj ./debug/Blink.doj $(VDSP)/211xx/lib/libc160.dlb $(VDSP)/211xx/lib/libio.dlb $(VDSP)/211xx/lib/libdsp160.dlb $(VDSP)/211xx/lib/libcpp.dlb $(VDSP)/211xx/lib/libeh.dlb $(VDSP)/211xx/lib/libcpprt.dlb 
	@echo "Linking..."
	$(VDSP)/cc21k.exe .\debug\benchmark.doj .\debug\Blink.doj -L .\debug -flags-link -od,. -o .\Blinkall.dxe -proc ADSP-21160 -MM

